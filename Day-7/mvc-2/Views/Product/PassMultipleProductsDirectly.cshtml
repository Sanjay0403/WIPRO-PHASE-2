@model List<MVCAssign.Models.Product>

<h2>Product List (Directly Passed)</h2>
<table border="1">
    <tr>
        <th>PCode</th>
        <th>Name</th>
        <th>Stock</th>
        <th>Details</th>
        <th>Price</th>
    </tr>
    @foreach (var product in Model)
    {
        <tr>
            <td>@product.PCode</td>
            <td>@product.Name</td>
            <td>@product.QtyInStock</td>
            <td>@product.Details</td>
            <td>$@product.Price</td>
        </tr>
    }
</table>
