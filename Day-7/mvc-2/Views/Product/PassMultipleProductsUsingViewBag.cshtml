@{
    var products = ViewBag.Products as List<MVCAssign.Models.Product>;
}

<h2>Product List (Using ViewBag)</h2>
@if (products != null && products.Count > 0)
{
    <table border="1">
        <tr>
            <th>PCode</th>
            <th>Name</th>
            <th>Stock</th>
            <th>Details</th>
            <th>Price</th>
        </tr>
        @foreach (var product in products)
        {
            <tr>
                <td>@product.PCode</td>
                <td>@product.Name</td>
                <td>@product.QtyInStock</td>
                <td>@product.Details</td>
                <td>$@product.Price</td>
            </tr>
        }
    </table>
}
